<h1><%= @picture.name %></h1>
<%= link_to "back", @picture.album%>
<div>
<h2>Description:</h2>
<p><%= @picture.description %></p>
<%= image_tag @picture.image_url(:post) if @picture.image? %>
<p>
	<%= link_to "Edit", edit_picture_path(@picture) %>
	<%= link_to "Delete", @picture, :method => :delete %></p>
</p>
</div>